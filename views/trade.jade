doctype html 5
html
    head
        meta(http-equiv="Content-Type", content="text/html; charset=UTF-8")
        meta(name="viewport", content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no")
        title 土鸡成交记录#{pageTxt} - 贵溪土鸡
        meta(name="keywords", content="出售贵溪土鸡,出售贵溪土鸡蛋")
        meta(name="description", content="出售正宗农村散养土鸡，贵溪土鸡蛋保证品质，保证营养，保证绿色欢迎前来购买。")
        link(rel="canonical", href="#{domain.pc + pageUrl}")
        include ../include/tongji.jade
    body
        .top-title
            h1 土鸡成交记录
        include ../include/menu.jade
        div.trade-list
            ul
                each item in trade
                    li
                        a(href="#{domain.m + '/detail/' + item.id}")
                            .diagram
                                    img(src="#{domain.static + '/img/' + item.type + item.logo}", alt="#{item.title}")
                            .cont
                                p.trade-title #{item.title}
                                p.essay #{item.depict}
                                p.biaoshi #{item.create_time}
            .page !{page}
        include ../include/footer.jade