doctype html 5
html
    head
        meta(http-equiv="Content-Type", content="text/html; charset=UTF-8")
        meta(name="viewport", content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no")
        title #{objData.title + ' - 贵溪土鸡'}
        meta(name="keywords", content="")
        meta(name="description", content="#{objData.title + ' - 贵溪土鸡每日分享农家生活、近日出售、站内新闻相关文章'}")
        link(rel="canonical", href="#{domain.pc + pageUrl}")
        meta(name="applicable-device", content="mobile")
        meta(http-equiv="Cache-Control", content="no-transform")
        meta(http-equiv="Cache-Control", content="no-siteapp")
        meta(name="MobileOptimized", content="width")
        meta(name="HandheldFriendly", content="true")
        meta(name="apple-mobile-web-app-capable", content="yes")
        meta(name="apple-mobile-web-app-status-bar-style", content="black")
        meta(name="apple-mobile-web-app-title", content="贵溪土鸡")
        link(rel="shortcut icon", href="/favicon.ico", type="image/x-icon")
        link(rel="stylesheet", type="text/css", href="/css/style_m.css?#{new Date().getTime()}")
    body
        .top-title.detail-tit
            a(href="javascript:history.back(-1)")
            h1 #{objData.title}
        p.curr-positon
            span 当前位置：
            a(href="#{domain.m + '/' +  objData.type + '/'}") #{typeTxt[objData.type]}
            span &nbsp;&gt;&nbsp;#{objData.title}
        .detail
            .jianjie
                p
                    span.lei 时间：
                    span #{objData.create_time}
                p
                    span.lei 来源：
                    span 贵溪土鸡网
                p
                    span.lei 编辑：
                    span 阿顺
                span.good-share 分享
            .cont !{objData.content}
        include ../include/footer.jade