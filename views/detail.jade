doctype html 5
html
    head
        meta(http-equiv="Content-Type", content="text/html; charset=UTF-8")
        title #{objData.title + ' - 贵溪土鸡'}
        meta(name="keywords", content="")
        meta(name="description", content="#{objData.title + ' - 贵溪土鸡每日分享农家生活、近日出售、站内新闻相关文章'}")
        link(rel="canonical", href="#{domain.pc + pageUrl}")
        //- link(rel="alternate", media="only screen and(max-width: 640px)", href="#{domain.m + pageUrl}")
        //- meta(name="mobile-agent", content="format=html5;url=#{domain.m + pageUrl}")
        meta(name="applicable-device", content="pc")
        meta(http-equiv="Cache-Control", content="no-transform")
        meta(http-equiv="Cache-Control", content="no-siteapp")
        meta(name="renderer", content="webkit")
        link(rel="stylesheet", type="text/css", href="#{domain.static}/css/style.css?#{new Date().getTime()}")
        link(rel="shortcut icon", href="#{domain.static}/img/favicon.ico", type="image/x-icon")
    body.life
        include ../include/header.jade
        p.curr-positon.width-1100
            span 当前位置：
            a(href="#{domain.pc}/") 首页
            span &nbsp;&gt;&nbsp;
            a(href="#{domain.pc + '/' +  objData.type + '/'}") #{typeTxt[objData.type]}
            span &nbsp;&gt;&nbsp;#{objData.title}
        div.detail.width-1100
            div.detail-cont
                h2 #{objData.title}
                p.texin 时间：#{objData.create_time}&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;来源：贵溪土鸡网&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;编辑：阿顺
                div.cont
                    p(style="line-height:20px") !{objData.content}
            div.news-tuijian
                div.column-title
                    span 推荐新闻
                    a(href="#{domain.pc + '/news/'}") 更多&gt;
                ul
                    each item,idx in newsList
                        li(class="#{idx == 5? 'active' : ''}")
                            i
                            a(href="#{domain.pc + '/detail/' + item.id}") #{item.title}
        include ../include/footer.jade
